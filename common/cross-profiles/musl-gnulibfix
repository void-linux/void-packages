#!/bin/sh
empty_file() {
        rm -f "$1"
        touch "$1"
}

dir="$1"
# fix files breaking the build entirely
for i in fseeko.c freadahead.c fseterr.c ; do empty_file "$dir"/$i ; done
echo "void close_stdin(void) {}" > "$dir"/closein.c

# fix stuff trying to reimplement libc
culprits=`cat << EOF
accept4
acosl
alloca
alphasort
asinl
asprintf
atanl
atexit
atoll
bcopy
btowc
chown
closedir
cosl
