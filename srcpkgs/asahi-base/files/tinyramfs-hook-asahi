for _mod in \
	apple-mailbox \
	nvme_apple \
	pinctrl-apple-gpio \
	macsmc macsmc-rtkit \
	i2c-apple i2c-pasemi-platform tps6598x apple-dart dwc3 dwc3-of-simple \
	nvme-apple-efuses phy-apple-atc xhci-plat-hcd xhci-pci pcie-apple \
	gpio_macsmc \
	rtc-macsmc spmi-apple-controller apple_nvmem_spmi \
	spi-apple spi-hid-apple spi-hid-apple-of \
	apple-dockchannel dockchannel-hid apple-rtkit-helper \
	apple-sio \
	mux-apple-display-crossbar phy-apple-dptx
do
	copy_kmod "$_mod"
done

for _bin in umount cpio cp grep cat sed seq
do
	copy_exec "$_bin"
done
