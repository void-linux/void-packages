tesseract-ocr-pus_package() {
	noarch=yes
	depends="${sourcepkg}>=${version}_${revision}"
	short_desc+=" - Pushto language data"
	pkg_install() {
		$(pkg_lang ${pkgname#tesseract-ocr-})
	}
}
