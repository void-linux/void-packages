-    print("sip_bin:%s" % shutil.which("sip5"))
+
+    # temporary sip6 / sip4 patch
+    # -> use sip4 sip binary
+    if sipbuild.version.SIP_VERSION >= 0x60000:
+        sip_bin = shutil.which("sip") 
