maintainer="k4leg <d0xi@inbox.ru>"
license="CC-BY-SA-4.0"
homepage="https://github.com/linuxmint/${pkgname}"
distfiles="http://packages.linuxmint.com/pool/main/m/${pkgname}/${pkgname}_${version}.tar.xz"
