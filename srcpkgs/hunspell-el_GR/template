# Template file for 'hunspell-el_GR'
pkgname=hunspell-el_GR
version=20110903
revision=1
archs=noarch
create_wrksrc=yes
hostmakedepends="bzip2"
short_desc="Greek el_GR dictionary for hunspell"
maintainer="Nicholas Christopoulos <nereus@freemail.gr>"
license="GPL-2.0"
homepage="https://sourceforge.net/projects/grspell/"
distfiles="${SOURCEFORGE_SITE}/grspell/hunspell-gr/${version}.tar.bz2"
checksum=e915481889b5a5049d0ab0f3e7e3681aafa0eda0c413b50fc9ee7d6fc683f8e8

do_install() {
	vinstall ${version}/el_GR.aff 644 /usr/share/hunspell
	vinstall ${version}/el_GR.dic 644 /usr/share/hunspell
}
