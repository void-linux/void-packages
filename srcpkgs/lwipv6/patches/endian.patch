Source: Debian

This fixes build on big endian.

--- a/lwip-v6/src/include/ipv6/lwip/inet.h
+++ b/lwip-v6/src/include/ipv6/lwip/inet.h
@@ -52,6 +52,8 @@
 #ifndef __LWIP_INET_H__
 #define __LWIP_INET_H__
 
+#include <arpa/inet.h>
+
 #include "lwip/arch.h"
 
 #include "lwip/opt.h"
@@ -80,21 +82,6 @@
 
 /*--------------------------------------------------------------------*/
 
-#ifndef htons
-
-#if BYTE_ORDER == BIG_ENDIAN
-#define htons(x) (x)
-#define ntohs(x) (x)
-#define htonl(x) (x)
-#define ntohl(x) (x)
-#else
-u16_t htons(u16_t x);
-u16_t ntohs(u16_t x);
-u32_t htonl(u32_t x);
-u32_t ntohl(u32_t x);
-#endif /* BYTE_ORDER == LITTLE_ENDIAN */
-
-#endif
 #endif /* __LWIP_INET_H__ */
 
 
