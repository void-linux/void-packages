+package_dir =
+	=src
+setup_requires =
+	setuptools
+	setuptools-scm
+
+[options.entry_points]
+console_scripts = 
+	gitchangelog = gitchangelog.gitchangelog:main
+
+[options.package_data]
+gitchangelog =
+	gitchangelog.rc.*
+	templates/**/*
