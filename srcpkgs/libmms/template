# Template file for 'libmms'
pkgname=libmms
version=0.6
distfiles="${SOURCEFORGE_SITE}/$pkgname/$pkgname-$version.tar.gz"
build_style=gnu_configure
short_desc="MMS stream protocol library"
maintainer="Juan RP <xtraeme@gmail.com>"
