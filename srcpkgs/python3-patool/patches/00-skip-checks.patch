+    """Extract an ACE archive."""
+    cmdlist = [cmd, 'x']
+    if not outdir.endswith('/'):
+        outdir += '/'
+    cmdlist.extend([archive, outdir])
+    return cmdlist
+
+def list_ace (archive, compression, cmd, verbosity, interactive):
+    """List an ACE archive."""
+    cmdlist = [cmd]
+    if verbosity > 1:
+        cmdlist.append('v')
+    else:
+        cmdlist.append('l')
+    cmdlist.append(archive)
+    return cmdlist
